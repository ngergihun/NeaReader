/*QSPINBOX-----------------------------------------------------------------------*/
QSpinBox,
QDoubleSpinBox,
QTextEdit,
QLineEdit,
QPushButton {
  border: 1px solid rgb(53, 94, 88);
  border-radius: 4px;
}
/*QTOOLBOX-----------------------------------------------------------------------*/
QToolBox::tab {
  background-color: rgb(43, 101, 117);
  color: #fff;
  border-radius: 8px;
  padding-left: 5px;
}
QToolBox::tab:selected,
QToolBox::tab:hover {
  background-color: rgb(31, 73, 84);
  border: 2px solid  rgb(65, 152, 176);
}

/*Labels-----------------------------------------------------------------------*/
QLabel#correctionsLabel,
QLabel#headerLabel {
  color: #0f1925;
  font-size: 18px;
  background-color: #fff;
}
QLabel#bgDegreeXLabel,
QLabel#bgDegreeYLabel {
  color: #0f1925;
  font-size: 14px;
  background-color: #fff;
}

QLabel#phaseRotLabel, 
QLabel#selfRefOrderLabel,
QLabel#lineLevelLabel {
  color: #0f1925;
  font-size: 14px;
  background-color: #fff;
}

QLabel#measinfo_label {
  color: #fff;
  font-weight: 600;
  background-color: rgb(65, 152, 176);
  border-radius: 8px;
}
QPushButton {
  background-color: #0d6efd;
  color: #fff;
  font-weight: 600;
  border-radius: 8px;
  border: 1px solid #0d6efd;
  padding: 5px 5px;
  margin-top: 0px;
  outline: 0px;
}
QPushButton:hover{
  background-color: #0b5ed7;
  border: 2px solid #9ac3fe;
}
QPushButton#prevButton,
QPushButton#nextButton {
  background-color: rgb(53, 94, 88);
  border: 1px solid rgb(53, 94, 88);
}
QPushButton#prevButton::hover,
QPushButton#nextButton::hover{
  background-color: rgb(34, 61, 57);
  border: 2px solid rgb(91, 166, 155);
}
QComboBox {
    border: 1px solid gray;
    border-radius: 3px;
    padding: 1px 18px 1px 3px;
    min-width: 6em;
}
QGroupBox {
    border: 2px solid gray;
    border-color: #FF17365D;
    margin-top: 10px;
    font-size: 12px;
    border-radius: 8px
}
QGroupBox::title {
	subcontrol-position: top center;
    border-radius: 2px;
	padding: 5px;
    background-color: #FF17365D;
    color: rgb(255, 255, 255);
}
QLabel {
  color: #fff;
  font-weight: 600;
  background-color: rgb(53, 94, 88);
  border-radius: 8px;
}
QTreeWidget {
  border: 2px solid gray;
  padding: 5px;
  border-radius: 8px;
  border-color: rgb(65, 152, 176);
}
QFrame#bgfit_toolsFrame,
QFrame#level_toolsFrame {
  border: 1px solid gray;
  padding: 5px;
  border-radius: 8px;
  border-color: rgb(53, 94, 88);
